<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.louis.ssm.dao.ActorDao">
    <select id="getAllActors" resultType="com.louis.ssm.bean.Actor">
        select * from actor
    </select>

    <select id="getActorById" resultType="com.louis.ssm.bean.Actor">
        select * from actor where actor_id = #{actorId}
    </select>
</mapper>